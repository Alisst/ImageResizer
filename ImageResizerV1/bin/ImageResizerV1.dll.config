  <system.webServer>
    <modules runAllManagedModulesForAllRequests="false">
      <add name="ImageResizingModule" type="ImageResizer.InterceptModule"/>
    </modules>
    <validation validateIntegratedModeConfiguration="false"/>
    <directoryBrowse enabled="true" />
  </system.webServer>

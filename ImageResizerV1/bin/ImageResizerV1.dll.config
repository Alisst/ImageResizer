      <add name="ImageResizingModule" type="ImageResizer.InterceptModule"/>
    </httpModules>
    <pages controlRenderingCompatibilityVersion="4.0">
      <controls>
        <add tagPrefix="da" namespace="ImageResizeController" assembly="ImageResizeController"/>
